# Copy this file to .env and fill in values for local development
# Required
MONGO_URI=
REDIS_URL=

# JWT secrets (development defaults provided if not set)
JWT_SECRET=dev_jwt_secret
JWT_REFRESH_SECRET=dev_refresh_secret

# Token expiry
ACCESS_EXPIRES_IN=1h
REFRESH_EXPIRES_IN=7d

# Optional: cloudinary, S3, other service keys
#CLOUDINARY_URL=
#S3_ENDPOINT=